//practical no 12...(code 1)
//use of public inheritance and checking accessibility of parent class methods in base class.
#include<iostream>
using namespace std;
 
class  A{

public:
int x;

protected:
int y;

private:
int z;

};

class B: public A{

public:
B()
{
x=21;
y=22;
//z=23;//error
}

void display(){
cout<<"x = "<<x<<endl<<"y = "<<y<<endl;//since 'z' cannot be used in this child class so printing is also not possible...
}
};
int main()
{
B o;
o.display();
}
