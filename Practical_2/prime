#include<iostream>
using namespace std;
int main()
{
        int n;
        cout<<"enter n : ";
        cin>>n;
        int i;
        for(i=2;i*i<=n;i++)
        {
                
                if(n%i==0)
                {cout<<"\nNot a Prime Number\n";
                break;
                }
                
        }
        if((i*i)>n)
        {
                cout<<"\nNumber is Prime\n";
                cout<<endl;
        }
        return 0;
}

